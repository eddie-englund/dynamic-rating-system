{
  "name": "drs",
  "version": "0.0.1",
  "description": "a dynamic self-hostable rating system",
  "main": "dist/index.js",
  "repository": "https://github.com/eddie-englund/dynamic-rating-system.git",
  "author": "Eddie Englund <eddie.englund@mingodesk.com>",
  "license": "GPL-3.0-or-later",
  "private": false,
  "scripts": {
    "deploy": "node ./dist/index.js",
    "dev": "nodemon --exec ts-node --files src/index.ts",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
    "ext": "ts"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.7.18",
    "nodemon": "^2.0.19",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "argon2": "^0.29.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.9.1",
    "ms": "^2.1.3",
    "tslib": "^2.4.0",
    "winston": "^3.8.2",
    "winston-console-format": "^1.0.8",
    "zod": "^3.19.1"
  }
}
